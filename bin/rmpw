#!/usr/bin/env python

import pw.cli


class RmPwHelper(pw.cli.CliHelper):

    def process_input(self, cli_input, name):

        if name:
            self.cli.data.pop(name, None)
            self.cli.save()
            print '{} removed'.format(name)
        else:
            print 'not found'

#
# Main program
#
pw.cli.Cli('Remove credentials.').run('remove', RmPwHelper())
